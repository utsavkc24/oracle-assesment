package com.librarymanagment.Library.repository;

import java.util.HashMap;

import com.librarymanagment.Library.model.Book;

public class BookRepository {

	private HashMap<Integer, Book> db;

	public BookRepository(HashMap<Integer, Book> db) {
		super();
		this.db = db;
	}

	public HashMap<Integer, Book> getDb() {
		return db;
	}
	
	
}
